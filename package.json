{
	"name": "react-stockcharts",
	"version": "0.7.8",
	"description": "Highly customizable stock charts with ReactJS and d3",
	"main": "src/index.js",
	"module": "src/index.js",
	"author": "Kossi D. T. Saka",
	"homepage": "http://rrag.github.io/react-stockcharts/",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf -rf ./build",
		"dev": "npm run clean && cross-env IP=0.0.0.0 PORT=8080 BABEL_ENV=watch webpack serve --env watch",
		"docs": "npm run build:docs && ncp docs/images/ build/images && ncp docs/data/ build/data",
		"build:docs": "echo 'DOCS - BUILD' && cross-env BABEL_ENV=es webpack --env docs",
		"prebuild": "npm run clean && npm run lint",
		"build": "npm run docs",
		"preview": "cd build && python3 -m http.server 8090",
		"babel:cjs:watch": "npm run babel:cjs -- -watch",
		"babel:es:watch": "npm run babel:es -- -watch",
		"babel:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir build --source-maps",
		"babel:es": "cross-env BABEL_ENV=es babel src --out-dir build/es --source-maps",
		"release": "npm run clean && npm run babel:cjs && npm run babel:es && cp README.md build/ && node scripts/release.js",
		"lint:fix": "eslint --ext .js,.jsx src --fix",
		"lint": "eslint --ext .js,.jsx src",
		"preversion": "npm run lint",
		"version": "node scripts/updateVersion.js && git add \"src/index.js\"",
		"postversion": "git push origin `git rev-parse --abbrev-ref HEAD` && git push origin `git describe`",
		"npm:publish:next": "npm run release && cd build && npm publish --tag next",
		"npm:publish:latest": "npm run release && cd build && npm publish",
		"sourcemap:visualize": "source-map-explorer build/dist/react-stockcharts.min.js build/dist/react-stockcharts.min.js.map"
	},
	"dependencies": {
		"@babel/eslint-parser": "^7.24.5",
		"d3-array": "^2.12.1",
		"d3-force": "^2.1.1",
		"d3-format": "^2.0.0",
		"d3-interpolate": "^2.0.1",
		"d3-path": "^2.0.0",
		"d3-scale": "^3.3.0",
		"d3-scale-chromatic": "^2.0.0",
		"d3-selection": "^2.0.0",
		"d3-shape": "^2.1.0",
		"d3-time": "^2.1.1",
		"d3-time-format": "^3.0.0",
		"debug": "^4.3.4",
		"lodash.flattendeep": "^4.4.0",
		"prop-types": "^15.8.1",
		"save-svg-as-png": "^1.4.17"
	},
	"peerDependencies": {
		"react": "^15.0.0 || ^16.0.0",
		"react-dom": "^15.0.0 || ^16.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.24.5",
		"@babel/preset-env": "^7.24.5",
		"@babel/preset-react": "^7.24.1",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^9.1.3",
		"bootstrap": "^5.3.3",
		"cross-env": "^7.0.3",
		"css-loader": "^6.11.0",
		"d3-dsv": "^2.0.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-flowtype": "^8.0.3",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"file-loader": "^6.2.0",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.6.0",
		"json-loader": "^0.5.7",
		"ncp": "^2.0.0",
		"postcss-loader": "^7.3.4",
		"prettier": "^3.2.5",
		"prismjs": "^1.29.0",
		"progress-bar-webpack-plugin": "^2.1.0",
		"react": "^16.14.0",
		"react-bootstrap": "^2.10.2",
		"react-dom": "^16.14.0",
		"remarkable-loader": "^0.2.1",
		"rimraf": "^5.0.5",
		"sass": "^1.77.0",
		"sass-loader": "^13.3.3",
		"shelljs": "^0.8.5",
		"shortid": "^2.2.16",
		"style-loader": "^3.3.4",
		"url-loader": "^4.1.1",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4",
		"webpack-config-utils": "^2.3.1",
		"webpack-dev-server": "^4.15.2"
	},
	"browserify": {
		"transform": [
			[
				"babelify",
				{
					"extensions": [
						".js",
						".jsx"
					]
				}
			]
		]
	},
	"keywords": [
		"charts",
		"charting",
		"stockcharts",
		"finance",
		"financechart",
		"react",
		"ReactJS",
		"d3",
		"react-component",
		"react-charts",
		"chart-components",
		"javascript-finance-chart",
		"javascript-stockchart",
		"data-visualization",
		"data-viz",
		"d3-finance-chart",
		"d3-stockchart"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kossidts/react-stockcharts.git"
	},
	"bugs": {
		"url": "https://github.com/kossidts/react-stockcharts/issues"
	}
}
